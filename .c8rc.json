{
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "exclude": [
    "out/test/**",
    "src/test/**",
    "**/*.test.ts",
    "**/*.test.js",
    "**/node_modules/**",
    "coverage/**",
    "dist/**"
  ],
  "include": [
    "out/src/**/*.js",
    "src/**/*.ts"
  ],
  "all": true,
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 70,
  "statements": 80,
  "reports-dir": "coverage"
}